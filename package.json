{
  "name": "@strawbinary-io/stripe-strapi-plugin",
  "version": "0.0.1",
  "description": "A powerful Strapi plugin to easily manage and sync your Stripe Products, Prices, Coupons and more â€“ directly in your project.",
  "keywords": [
    "strapi",
    "stripe",
    "strapi5",
    "strapi-cms",
    "cms",
    "headlesscms"
  ],
  "homepage": "https://strawbinary.de/",
  "bugs": {
    "url": "https://github.com/Strawbinary/stripe-strapi-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Strawbinary/stripe-strapi-plugin.git"
  },
  "license": "MIT",
  "author": "Strawbinary <info@strawbinary.de>",
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "release": "commit-and-tag-version"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "react-intl": "^7.1.14",
    "stripe": "^19.2.0"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.30.0",
    "@strapi/typescript-utils": "^5.30.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "commit-and-tag-version": "^12.6.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "stripe-strapi-plugin",
    "displayName": "Stripe Plugin",
    "description": ""
  }
